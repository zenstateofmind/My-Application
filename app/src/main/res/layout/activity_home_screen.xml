<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/home_screen_background"
    tools:context=".HomeScreen">

    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:name="com.timeto.makemezen.CalendarViewFragment"
        android:id="@+id/calendar_view_fragment_container"/>


    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.timeto.makemezen.AppUsageListFragment"
        android:id="@+id/app_usage_list_fragment"/>



</LinearLayout>